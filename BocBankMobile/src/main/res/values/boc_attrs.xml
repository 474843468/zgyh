<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="psdTextView">
        <attr name="isPasswordAppear" format="boolean"/>
        <attr name="passwordLength" format="integer"/>
    </declare-styleable>

    <declare-styleable name="itemSelectorView">
        <attr name="name" format="string"/>
        <attr name="firstText" format="string"/>
        <attr name="secondText" format="string"/>
    </declare-styleable>

    <declare-styleable name="BocConvenientBanner">
        <attr name="canLoop" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="editChoiceWidget">
        <attr name="editChoiceName" format="string"/>
        <attr name="editChoiceNameBold" format="boolean"/>
        <attr name="isVisibilityLeftIcon" format="boolean"/>
        <attr name="leftIconImageId" format="integer"/>
        <attr name="editChoiceContent" format="string"/>
        <attr name="moneyInputMaxLeftNum" format="integer"/>
        <attr name="moneyInputMaxRightNum" format="integer"/>
    </declare-styleable>

    <declare-styleable name="editClearWidget">
        <attr name="isShowWidgetImage" format="boolean"/>
        <attr name="isShowWidgetRightImage" format="boolean"/>
        <attr name="isNameTextBold" format="boolean"/>
        <attr name="editClearName" format="string"/>
        <attr name="editClearHint" format="string"/>
        <attr name="editClearImage" format="integer"/>
        <attr name="editClearRightImage" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SelectGridView">
        <attr name="radioTrue" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="moneyInputTextView">
        <attr name="maxLeftNum" format="integer"/>
        <attr name="maxRightNum" format="integer"/>
        <attr name="currency" format="string"/>
    </declare-styleable>

    <!--详情组件底部按钮-->
    <declare-styleable name="detailsButton">
        <attr name="textTitle" format="string"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>
    <!--详情组件-->
    <declare-styleable name="detailsTextView">
        <attr name="textName" format="string"/>
        <attr name="textValue" format="string"/>
        <attr name="fullLine" format="boolean"/>
        <attr name="lineVisibility" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DetailTableRowGroup2">
        <attr name="groupTitle" format="string"/>
    </declare-styleable>

    <!-- 快速索引条 -->
    <declare-styleable name="QuickIndexBar">
        <!-- 索引条按下时的背景颜色 -->
        <attr name="bgColorPress" format="color"/>
        <!-- 索引条正常状态时（未按下时）的背景颜色 -->
        <attr name="bgColorNormal" format="color"/>
        <!-- 索引文本大小 -->
        <attr name="indexTextSize" format="dimension"/>
        <!-- 索引文本颜色 -->
        <attr name="indexTextColor" format="color"/>
        <!-- 索引文本颜色 -->
        <attr name="indexTextColorPress" format="color"/>
        <!-- 头部的索引文字，以逗号分隔 -->
        <attr name="strArr" format="string" />
    </declare-styleable>

    <attr name="autoScaleTextViewStyle" format="reference"/>
    <declare-styleable name="AutoScaleTextView">
        <attr name="minTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference"/>
        <attr name="paused" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifMoviewViewStyle" format="reference"/>
    </declare-styleable>

    <!-- 还款计划Tab页 -->
    <declare-styleable name="TableLabelView">
        <attr name="tableLabelContentText" format="string"/>
    </declare-styleable>

    <!--圆形带百分比进度条-->
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension" />
        <attr name="progressTextColor" format="color"/>
        <attr name="progressTextSize" format="dimension"/>
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StickyListHeadersListView">
        <attr name="stickyListHeadersListViewStyle" format="reference"/>

        <!-- View attributes -->
        <attr name="android:clipToPadding"/>
        <attr name="android:scrollbars"/>
        <attr name="android:overScrollMode"/>
        <attr name="android:padding"/>
        <attr name="android:paddingLeft"/>
        <attr name="android:paddingTop"/>
        <attr name="android:paddingRight"/>
        <attr name="android:paddingBottom"/>

        <!-- ListView attributes -->
        <attr name="android:fadingEdgeLength"/>
        <attr name="android:requiresFadingEdge"/>
        <attr name="android:cacheColorHint"/>
        <attr name="android:choiceMode"/>
        <attr name="android:drawSelectorOnTop"/>
        <attr name="android:fastScrollEnabled"/>
        <attr name="android:fastScrollAlwaysVisible"/>
        <attr name="android:listSelector"/>
        <attr name="android:scrollingCache"/>
        <attr name="android:scrollbarStyle"/>
        <attr name="android:divider"/>
        <attr name="android:dividerHeight"/>
        <attr name="android:transcriptMode"/>
        <attr name="android:stackFromBottom"/>

        <!-- StickyListHeaders attributes -->
        <attr name="hasStickyHeaders" format="boolean"/>
        <attr name="isDrawingListUnderStickyHeader" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ViewPagerIndicator">

        <!-- Style of the tab indicator's tabs. -->
        <attr name="TabPageIndicatorStyle" format="reference"/>
        <!-- Style of the underline indicator. -->
        <attr name="UnderlinePageIndicator" format="reference"/>
    </declare-styleable>

    <attr name="centered" format="boolean"/>
    <attr name="selectedColor" format="color"/>
    <attr name="strokeWidth" format="dimension"/>
    <attr name="unselectedColor" format="color"/>

    <declare-styleable name="UnderlinePageIndicator">

        <!-- Whether or not the selected indicator fades. -->
        <attr name="fades" format="boolean"/>
        <!-- Length of the delay to fade the indicator. -->
        <attr name="fadeDelay" format="integer"/>
        <!-- Length of the indicator fade to transparent. -->
        <attr name="fadeLength" format="integer"/>
        <!-- Color of the selected line that represents the current page. -->
        <attr name="selectedColor"/>
    </declare-styleable>
    <!--饼图-属性-2016-9-12 16:22:27-->
    <declare-styleable name="Cercle">
        <attr name="pieRoundColor" format="color"></attr>
        <attr name="pieRoundProgressColor" format="color"></attr>
        <attr name="pieRoundWidth" format="dimension"></attr>
        <attr name="pieTextColor" format="color"></attr>
        <attr name="pieTextSize" format="dimension"></attr>
        <attr name="pieMax" format="integer"></attr>
        <attr name="pieTextlsDisplayable" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="ClickDownShowView">
        <attr name="txtName" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="SelectTypeView">
        <attr name="addHeadView" format="boolean"></attr>
    </declare-styleable>

    <!--圆环进度条-->
    <declare-styleable name="CircleProgressBar">
        <!--圆边框颜色-->
        <attr name="circle_stroke_color" format="color" />
        <!--圆的半径-->
        <attr name="circle_radius" format="dimension" />
        <!--进度条颜色-->
        <attr name="progress_color" format="color" />
        <!--进度条宽度-->
        <attr name="progress_width" format="dimension" />
        <!--文字颜色-->
        <attr name="text_color" format="color" />
        <!--文字大小-->
        <attr name="text_size" format="dimension" />
        <!--秒字大小-->
        <attr name="text_second_size" format="dimension" />
    </declare-styleable>

    <!--环形菜单item-->
    <declare-styleable name="BaseCircleMenuItemView">
        <attr name="titileName" format="string"/>
        <attr name="content" format="string"/>
    </declare-styleable>


    <!-- 跨境专区Tab页 -->
    <declare-styleable name="OSTableLabelView">
        <attr name="ostableLabelContentText" format="string"/>
    </declare-styleable>
    <!-- 信用卡选择期数网格按钮 -->
    <declare-styleable name="GridButton">
        <attr name="GridButton_numColumns" format="integer" />
        <attr name="GridButton_btnNames" format="integer" />
        <attr name="GridButton_item" format="integer" />
        <attr name="GridButton_normal_bg" format="integer" />
        <attr name="GridButton_selected_bg" format="integer" />
    </declare-styleable>

    <!-- 四方k线图组件 -->
    <declare-styleable name="ChartsView">
        <attr name="menu_margin" format="integer" />
        <attr name="menu_child_width" format="integer" />
        <attr name="menu_child_background" format="reference|color" />
        <attr name="menu_child_alpha" format="float" />
        <attr name="more_button" format="reference" />
        <attr name="fold_button" format="reference" />
        <attr name="left_arrow" format="reference" />
        <attr name="right_arrow" format="reference" />
        <attr name="zoom_in" format="reference" />
        <attr name="zoom_out" format="reference" />
    </declare-styleable>
    <!--上拉下拉布局-->
    <declare-styleable name="PullAndPushLayout">
        <attr name="refreshViewDrawable" format="reference" />
        <attr name="loadingDrawable" format="reference" />
        <attr name="defaultNotice" format="string" />
        <attr name="pullNotice" format="string" />
        <attr name="refreshingNotice" format="string" />
        <attr name="pushNotice" format="string" />
        <attr name="successNotice" format="string" />
        <attr name="failNotice" format="string" />
    </declare-styleable>
    <!--信用卡卡列表-->
    <declare-styleable name="CardStackView">
        <attr name="stackHeaderHeight" format="dimension"/>
        <attr name="stackDuration" format="integer"/>
        <attr name="stackOverlapGapsCollapse" format="dimension"/>
        <attr name="stackOverlapGaps" format="dimension"/>
    </declare-styleable>

    <!--带开关的ItemView-->
    <declare-styleable name="ToggleRowView">
        <attr name="toggleOn" format="boolean"/>
        <attr name="toggleTitle" format="reference|string"/>
    </declare-styleable>
</resources>