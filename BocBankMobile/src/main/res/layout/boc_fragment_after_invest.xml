<?xml version="1.0" encoding="UTF-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    android:paddingTop="@dimen/boc_space_between_470px"
    android:background="@drawable/oversea_ii_after_bg">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/exchange_settlement"
            android:layout_alignParentTop="true"
            style="@style/OverseaszoneNormalTv"
            />

        <TextView
            android:id="@+id/exchange_purchasing"
            android:layout_below="@+id/exchange_settlement"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv" />

        <TextView
            android:id="@+id/remit"
            android:layout_below="@+id/exchange_purchasing"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv" />

        <TextView
            android:id="@+id/apply_credit_card"
            android:layout_below="@+id/remit"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/inward"
            android:layout_below="@+id/apply_credit_card"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/command"
            android:layout_below="@+id/inward"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/Invest_card1"
            android:layout_below="@+id/command"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv"/>

        <TextView
            android:id="@+id/invest_card2"
            android:layout_below="@+id/command"
            android:layout_toRightOf="@+id/Invest_card1"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/invest_card3"
            android:layout_below="@+id/Invest_card1"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv"/>

        <TextView
            android:id="@+id/invest_card4"
            android:layout_below="@+id/Invest_card1"
            android:layout_toRightOf="@+id/invest_card3"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/more_service"
            android:layout_below="@+id/invest_card3"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/foreign_currency_permit"
            android:layout_below="@+id/more_service"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/abroad_personal_loan_sg"
            android:layout_below="@+id/more_service"
            android:layout_toRightOf="@+id/foreign_currency_permit"
            style="@style/OverseaszoneTv" />
        <TextView
            android:id="@+id/abroad_personal_loan_my"
            android:layout_below="@+id/foreign_currency_permit"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />
        <TextView
            android:id="@+id/abroad_personal_loan_ca"
            android:layout_toRightOf="@+id/abroad_personal_loan_my"
            android:layout_below="@+id/foreign_currency_permit"
            android:layout_marginRight="@dimen/boc_space_between_20px"
            style="@style/OverseaszoneTv" />
        <TextView
            android:id="@+id/abroad_personal_loan_uk"
            android:layout_below="@+id/abroad_personal_loan_my"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/credit_information_service"
           android:layout_toRightOf="@+id/abroad_personal_loan_uk"
            android:layout_below="@+id/abroad_personal_loan_ca"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/malaysia_plan"
            android:layout_below="@+id/abroad_personal_loan_uk"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/boc_space_between_20px"
            android:layout_below="@+id/malaysia_plan"/>

    </RelativeLayout>

</ScrollView>
