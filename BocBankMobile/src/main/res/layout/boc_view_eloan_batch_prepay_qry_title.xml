<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/boc_bg_eloan_batch_prepay_qry_desc"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tvSelectDesc"
        style="@style/tv_small"
        android:paddingLeft="@dimen/boc_space_between_30px"
        android:paddingBottom="@dimen/boc_space_between_20px"
        android:paddingTop="@dimen/boc_space_between_36px"
        android:textColor="@color/boc_text_color_dark_gray"
        android:textStyle="bold"
        android:text="@string/boc_common_select"/>
    <TextView
        android:id="@+id/tvMaxSel"
        style="@style/tv_small"
        android:paddingLeft="@dimen/boc_space_between_6px"
        android:paddingBottom="@dimen/boc_space_between_20px"
        android:paddingTop="@dimen/boc_space_between_40px"
        android:textColor="@color/boc_text_color_common_gray"
        android:text=""/>
</LinearLayout>
