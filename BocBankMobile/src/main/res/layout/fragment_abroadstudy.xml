<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:id="@+id/rl_container"
    android:orientation="vertical">
    <com.boc.bocsoft.mobile.bocmobile.buss.specializedservice.overseaszone.base.NoScrollViewPager
        android:id="@+id/vpContent1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.boc.bocsoft.mobile.bocmobile.buss.specializedservice.overseaszone.base.NoScrollViewPager>

    <include layout="@layout/overseas_fragment_title"
        android:id="@+id/viewTitle1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/boc_titlebar_height"/>
    <ImageView
       android:id="@+id/strategy"
        style="@style/OverseaszoneStrategy"/>

</RelativeLayout>