<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/boc_common_bg_color">
    <LinearLayout
        android:id="@+id/lv_TaDetail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/boc_popdialog_padding_20px"
        android:orientation="vertical">

        <View
            android:id="@+id/divider_top"
            style="@style/divider_horizontal"
            android:layout_alignParentTop="true" />

        <com.boc.bocsoft.mobile.bocmobile.base.widget.details.DetailRow
            android:id="@+id/fincAccount"
            android:layout_width="match_parent"
            android:layout_height="@dimen/boc_space_between_80px" />

        <com.boc.bocsoft.mobile.bocmobile.base.widget.details.DetailRow
            android:id="@+id/taAccount"
            android:layout_width="match_parent"
            android:layout_height="@dimen/boc_space_between_80px" />

        <com.boc.bocsoft.mobile.bocmobile.base.widget.details.DetailRow
            android:id="@+id/regOrgCode"
            android:layout_width="match_parent"
            android:layout_height="@dimen/boc_space_between_80px" />

        <View
            android:layout_height="@dimen/boc_popdialog_padding_20px"
            style="@style/divider_horizontal"
            android:background="@color/transparent"></View>

        <Button
            android:id="@+id/btn_confirm"
            style="@style/BocButton.BocPrimaryButton"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/boc_space_between_40px"
            android:layout_marginTop="@dimen/boc_space_between_50px"
            android:text="@string/boc_common_sure"/>

    </LinearLayout>

</FrameLayout>
