<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/boc_common_cell_color"
    android:orientation="vertical">

    <TextView
        android:id="@+id/total_head"
        style="@style/BocTextViewSmall"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/boc_space_between_30px"
        android:gravity="center"
        android:text="总结"
        android:textColor="@color/boc_text_mobile_color"/>

    <TextView
        android:id="@+id/total_account"
        style="@style/BocTextViewMoney"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/boc_space_between_30px"
        android:gravity="center"
        android:text="总结"
        android:textColor="@color/boc_text_mobile_color"/>
    <include
        layout="@layout/boc_divide_line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/boc_divider_1px"
        android:layout_marginLeft="@dimen/boc_space_between_20px"
        android:layout_marginRight="@dimen/boc_space_between_20px"/>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/boc_space_between_16px"
        android:paddingBottom="@dimen/boc_space_between_20px">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/boc_space_between_10px"
            android:paddingBottom="@dimen/boc_space_between_10px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/left_top"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/boc_fund_profileloss_realize_profile_loss"
                android:textColor="@color/boc_text_mobile_color">

            </TextView>

            <TextView
                android:id="@+id/right_top"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/boc_fund_profileloss_position_profile_loss"
                android:textColor="@color/boc_text_mobile_color">

            </TextView>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/boc_space_between_10px"
            android:paddingBottom="@dimen/boc_space_between_20px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/left_bottom"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="100"
                android:textColor="@color/boc_text_mobile_color">

            </TextView>

            <TextView
                android:id="@+id/right_bottom"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="123"
                android:textColor="@color/boc_text_mobile_color">

            </TextView>
        </TableRow>
    </TableLayout>



</LinearLayout>