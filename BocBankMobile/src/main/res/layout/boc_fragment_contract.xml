<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/boc_main_bg_color">

    <Button
        android:id="@+id/btn_know"
        style="@style/BocButton.BocPromptButton"
        android:layout_alignParentBottom="true"
        android:text="@string/boc_contact_know"/>

    <com.boc.bocsoft.mobile.bocmobile.base.widget.webView.BaseContractWebView
        android:id="@+id/webview_contract"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/btn_know"/>
</RelativeLayout>