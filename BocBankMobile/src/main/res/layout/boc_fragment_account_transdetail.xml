<?xml version="1.0" encoding="utf-8"?>
<com.boc.bocsoft.mobile.bocmobile.base.widget.SlipMenu.SlipDrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/transdetail_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/boc_common_bg_color"
    >


    <com.boc.bocsoft.mobile.bocmobile.base.widget.refreshliseview.PullToRefreshLayout
        android:id="@+id/refresh_account_transdetail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.boc.bocsoft.mobile.bocmobile.base.widget.transactionlist.TransactionView
            android:id="@+id/lv_account_transaction"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <include layout="@layout/boc_load_more"/>
    </com.boc.bocsoft.mobile.bocmobile.base.widget.refreshliseview.PullToRefreshLayout>


    <com.boc.bocsoft.mobile.bocmobile.base.widget.selectview.selecttimerange.SelectTimeRange
        android:id="@+id/right_drawer_transdetail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="end"
        android:clickable="true"/>

</com.boc.bocsoft.mobile.bocmobile.base.widget.SlipMenu.SlipDrawerLayout>