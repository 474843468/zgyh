<?xml version="1.0" encoding="UTF-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id ="@+id/applyschool_scro"
    android:scrollbars="none"
    android:paddingTop="@dimen/boc_space_between_540px"
    android:background="@drawable/overseazone_beforevisafragment_bg"
    >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_deposit_service"
            android:layout_alignParentTop="true"
            style="@style/OverseaszoneNormalTv"
            />
        <TextView
            android:id="@+id/tv_exchange_purchasing"
            android:layout_below="@+id/tv_deposit_service"
            android:layout_marginTop="@dimen/boc_space_between_80px"
            style="@style/OverseaszoneNormalTv"
            />

        <TextView
            android:id="@+id/tv_student_loan"
            android:layout_below="@+id/tv_exchange_purchasing"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/tv_credit_card"
            android:layout_below="@+id/tv_student_loan"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv" />

        <TextView
            android:id="@+id/tv_recommendation"
            android:layout_below="@+id/tv_credit_card"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/tv_globle_zhuojun_card"
            android:layout_below="@+id/tv_recommendation"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/tv_allthingwell_card"
            android:layout_below="@+id/tv_recommendation"
            android:layout_toRightOf="@+id/tv_globle_zhuojun_card"
            style="@style/OverseaszoneTv"/>

        <TextView
            android:id="@+id/tv_multi_currency_card"
            android:layout_below="@+id/tv_globle_zhuojun_card"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/tv_study_card"
            android:layout_below="@+id/tv_globle_zhuojun_card"
            android:layout_toRightOf="@+id/tv_multi_currency_card"
            style="@style/OverseaszoneTv"/>

        <TextView
            android:id="@+id/tv_more_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_study_card"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            android:textSize="@dimen/boc_text_size_small" />

        <TextView
            android:id="@+id/tv_GIC_certification_of_deposit"
            android:layout_below="@+id/tv_more_service"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv"
            />

        <TextView
            android:id="@+id/tv_insurance_abroad_study_card"
            android:layout_below="@+id/tv_GIC_certification_of_deposit"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv"
            />
        <TextView
            android:id="@+id/tv_traveller_cheque"
            android:layout_below="@+id/tv_GIC_certification_of_deposit"
            android:layout_toRightOf="@+id/tv_insurance_abroad_study_card"
            style="@style/OverseaszoneTv"
            />
        <TextView
            android:id="@+id/tv_currency_carry_permit"
            android:layout_below="@+id/tv_insurance_abroad_study_card"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            android:layout_marginBottom="@dimen/boc_space_between_20px"
            style="@style/OverseaszoneTv"
            />
        <TextView
            android:id="@+id/tv_agent_account_witness"
            android:layout_below="@+id/tv_insurance_abroad_study_card"
            android:layout_toRightOf="@+id/tv_currency_carry_permit"
            style="@style/OverseaszoneTv"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/boc_space_between_20px"
            android:layout_below="@+id/tv_agent_account_witness"/>

    </RelativeLayout>
</ScrollView>