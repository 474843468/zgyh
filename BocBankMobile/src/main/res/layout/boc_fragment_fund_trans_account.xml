<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/boc_common_bg_color">
    <LinearLayout
        android:id="@+id/lv_transTaAccount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.boc.bocsoft.mobile.bocmobile.base.widget.details.DetailTableRowButton
            android:id="@+id/fincAccount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.boc.bocsoft.mobile.bocmobile.base.widget.details.DetailTableRowButton
            android:id="@+id/accountNo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.boc.bocsoft.mobile.bocmobile.base.widget.details.DetailRow
            android:id="@+id/qccBalanceA"
            android:layout_width="match_parent"
            android:layout_height="@dimen/boc_space_between_80px" />
        <!--换行后的高度待改-->
    </LinearLayout>
</FrameLayout>
