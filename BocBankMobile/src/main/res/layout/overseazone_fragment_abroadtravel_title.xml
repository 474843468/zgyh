<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_vertical">

    <ImageView
        android:id="@+id/leftIconIv"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:paddingLeft="@dimen/boc_space_between_24px"
        android:paddingRight="@dimen/boc_space_between_24px"
        android:src="@drawable/boc_back_white" />

    <com.boc.bocsoft.mobile.bocmobile.buss.specializedservice.overseaszone.base.OSTableLabelView
        android:id="@+id/tabLabeltravel"
        android:layout_width="@dimen/boc_space_between_400px"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@null"
        >
    </com.boc.bocsoft.mobile.bocmobile.buss.specializedservice.overseaszone.base.OSTableLabelView>

</RelativeLayout>