<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/boc_common_bg_color"
    android:orientation="vertical">
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/boc_divider_line_color"/>
    <com.boc.bocsoft.mobile.bocmobile.base.widget.edittext.EditChoiceWidget
    android:id="@+id/eloanApplyLl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:editChoiceName="@string/boc_eloan_quoteapply" />

    <com.boc.bocsoft.mobile.bocmobile.base.widget.edittext.EditChoiceWidget
        android:id="@+id/eloanQuoteLl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:editChoiceName="@string/boc_eloan_quotedetail" />


    <com.boc.bocsoft.mobile.bocmobile.base.widget.edittext.EditChoiceWidget
        android:id="@+id/eloanAccountLl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:editChoiceName="@string/boc_eloan_changeAccount" />

    <com.boc.bocsoft.mobile.bocmobile.base.widget.edittext.EditChoiceWidget
        android:id="@+id/eloanRapayLl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:editChoiceName="@string/boc_eloan_prepay" />

    <com.boc.bocsoft.mobile.bocmobile.base.widget.edittext.EditChoiceWidget
        android:id="@+id/eloanPay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:editChoiceName="@string/boc_eloan_paySetings" />


</LinearLayout>