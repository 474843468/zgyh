<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/boc_common_bg_color">

    <com.boc.bocsoft.mobile.bocmobile.base.widget.webView.ContractWebView
        android:id="@+id/user_limit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/btn_know"
        android:layout_below="@id/head" />

    <TextView
        android:id="@+id/btn_know"
        android:layout_width="match_parent"
        android:layout_height="@dimen/boc_button_height_88px"
        android:layout_alignParentBottom="true"
        android:background="@color/boc_main_button_color"
        android:gravity="center"
        android:text="我知道了"
        android:textColor="@color/boc_common_cell_color" />

</RelativeLayout>