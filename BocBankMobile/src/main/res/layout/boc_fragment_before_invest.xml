<?xml version="1.0" encoding="UTF-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/scroll_before"
    android:scrollbars="none"
    android:paddingTop="@dimen/boc_space_between_470px"
    android:background="@drawable/oversea_ii_before_bg">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/deposit_certification_service"
            style="@style/OverseaszoneNormalTv"
            />

        <TextView
            android:id="@+id/more_service"
            android:layout_below="@+id/deposit_certification_service"
            android:layout_marginTop="@dimen/boc_space_between_60px"
            style="@style/OverseaszoneNormalTv"/>

        <TextView
            android:id="@+id/abroad_consult_service"
            android:layout_below="@+id/more_service"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/tips"
            android:layout_below="@+id/abroad_consult_service"
            android:layout_marginTop="@dimen/boc_space_between_10px"
            style="@style/OverseaszoneNormalTv" />

        <TextView
            android:id="@+id/insurance_abroad_card"
            android:layout_below="@+id/tips"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/immigration_fund_supervision"
            android:layout_below="@+id/tips"
            android:layout_marginRight="@dimen/boc_space_between_50px"
            android:layout_toRightOf="@+id/insurance_abroad_card"
            style="@style/OverseaszoneTv" />

        <TextView
            android:id="@+id/agent_account_witness"
            android:layout_below="@+id/insurance_abroad_card"
            android:layout_marginLeft="@dimen/boc_space_between_50px"
            android:layout_marginRight="@dimen/boc_space_between_50px"
            android:layout_marginBottom="@dimen/boc_space_between_20px"
            style="@style/OverseaszoneTv"/>

    </RelativeLayout>

</ScrollView>
