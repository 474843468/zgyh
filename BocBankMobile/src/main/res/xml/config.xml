<?xml version='1.0' encoding='utf-8'?>
<widget id="com.xdy.cordovademo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaDemo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="com.boc.bocsoft.mobile.bocmobile.base.cordova.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="commonBridge">
        <param name="android-package" value="com.boc.bocsoft.mobile.bocmobile.base.cordova.CommonBridgePlugin" />
        <param name="onload" value="true" />
    </feature>

    <feature name="creditBridge">
        <param name="android-package" value="com.boc.bocsoft.mobile.bocmobile.buss.creditcard.webplugin.CreditPlugin" />
        <param name="onload" value="true" />
    </feature>
</widget>
