<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/boc_main_button_color"/>
            <stroke android:width="0.5dp" android:color="#b2b2b2"/>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/boc_common_cell_color"/>
        </shape>
    </item>
</selector>