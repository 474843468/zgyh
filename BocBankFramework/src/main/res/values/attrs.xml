<resources>
    <declare-styleable name="ScanBoxView">
        <!-- 扫描框的宽度-->
        <attr name="boxWidth" format="dimension|reference" />
        <!-- 扫描框的高度-->
        <attr name="boxHeight" format="dimension|reference" />
        <!-- 扫描框背景-->
        <attr name="boxBackground" format="reference" />
        <!-- 扫描线-->
        <attr name="scanLine" format="reference" />
        <!-- 扫描线topMargin-->
        <attr name="scanLineTopMargin" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="TitleBarView">
        <attr name="iconLeft" format="reference"/>
        <attr name="iconRight" format="reference"/>
        <attr name="titleColor" format="color|reference"/>
        <attr name="titleString" format="string|reference"/>
        <attr name="barBackground" format="color|reference"/>
    </declare-styleable>
</resources>
