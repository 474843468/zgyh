<resources>


    <declare-styleable name="NewLabelTextView">
        <attr name="NL_labelText" format="string" />
        <attr name="NL_valueText" format="string" />
        <attr name="android:ellipsize" />
       <attr name="NL_LableTextColor">
            <enum name="red" value="0"></enum>
            <enum name="black" value="1"></enum>
        </attr>
        <attr name="NL_ValueTextColor">
            <enum name="red" value="0"></enum>
            <enum name="black" value="1"></enum>
        </attr>
        <attr name="NL_weightShowRate" format="string" />
    </declare-styleable>


    <declare-styleable name="moneyInputTextView">
        <attr name="maxLeftNum" format="integer" />
        <attr name="maxRightNum" format="integer" />
    </declare-styleable>


    <declare-styleable name="BackgroundLayout">
        <attr name="leftText" format="string" />
        <attr name="rightText" format="string" />
        <attr name="titleText" format="string" />
    </declare-styleable>


    <declare-styleable name="TabView">
        <attr name="contentText" format="string"/>
    </declare-styleable>
    
</resources>