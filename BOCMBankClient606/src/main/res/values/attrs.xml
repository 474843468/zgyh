<resources>

    <attr name="centered" format="boolean" />
    <attr name="selectedColor" format="color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="unselectedColor" format="color" />

    <declare-styleable name="ViewPagerIndicator">

        <!-- Style of the circle indicator. -->
        <attr name="vpiCirclePageIndicatorStyle" format="reference" />
        <!-- Style of the tab indicator's tabs. -->
        <attr name="vpiTabPageIndicatorStyle" format="reference" />
        <!-- Style of the icon indicator's views. -->
        <attr name="vpiIconPageIndicatorStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="CirclePageIndicator">

        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" />
        <!-- Color of the filled circle that represents the current page. -->
        <attr name="fillColor" format="color" />
        <!-- Color of the filled circles that represents pages. -->
        <attr name="pageColor" format="color" />
        <!-- Orientation of the indicator. -->
        <attr name="android:orientation" />
        <!-- Radius of the circles. This is also the spacing between circles. -->
        <attr name="radius" format="dimension" />
        <!-- Whether or not the selected indicator snaps to the circles. -->
        <attr name="snap" format="boolean" />
        <!-- Color of the open circles. -->
        <attr name="strokeColor" format="color" />
        <!-- Width of the stroke used to draw the circles. -->
        <attr name="strokeWidth" />
        <!-- View background -->
        <attr name="android:background" />
    </declare-styleable>
    <declare-styleable name="SwipeListView">
        <attr name="swipeOpenOnLongPress" format="boolean" />
        <attr name="swipeAnimationTime" format="integer" />
        <attr name="swipeOffsetLeft" format="dimension" />
        <attr name="swipeOffsetRight" format="dimension" />
        <attr name="swipeCloseAllItemsWhenMoveList" format="boolean" />
        <attr name="swipeFrontView" format="reference" />
        <attr name="swipeBackView" format="reference" />
        <attr name="swipeMode" format="enum">
            <enum name="none" value="0" />
            <enum name="both" value="1" />
            <enum name="right" value="2" />
            <enum name="left" value="3" />
        </attr>
        <attr name="swipeActionLeft" format="enum">
            <enum name="reveal" value="0" />
            <enum name="dismiss" value="1" />
        </attr>
        <attr name="swipeActionRight" format="enum">
            <enum name="reveal" value="0" />
            <enum name="dismiss" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="UsbKeyText">
        <attr name="ShowRate" format="string" />
        <attr name="PaddLeft" format="string" />
        <attr name="MargRight" format="string" />
    </declare-styleable>
    <declare-styleable name="LinearListView">
        <attr name="android:entries" />
        <attr name="dividerThickness" format="dimension" />
    </declare-styleable>

    <attr name="weightShowRate" format="string" />

    <declare-styleable name="LabelTextView">
        <attr name="labelText" format="string" />
        <attr name="valueText" format="string" />
        <attr name="android:ellipsize" />
        <attr name="LableTextColor">
            <enum name="red" value="0"></enum>
            <enum name="black" value="1"></enum>
        </attr>
        <attr name="ValueTextColor">
            <enum name="red" value="0"></enum>
            <enum name="black" value="1"></enum>
        </attr>
        <attr name="weightShowRate" />
    </declare-styleable>
  <!--  <declare-styleable name="BackgroundLayout">
        <attr name="leftText" format="string" />
        <attr name="rightText" format="string" />
        <attr name="titleText" format="string" />
    </declare-styleable>-->
    <declare-styleable name="TitleAndContentLayout">
        <attr name="android:text" />
        <attr name="titleVisibility">
            <enum name="visibility" value="0" />
            <enum name="gone" value="1" />
            <enum name="inVisibility" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="TableLabelView">
        <attr name="tableLabelContentText" format="string" />
    </declare-styleable>
    <declare-styleable name="ListTitleView">
        <attr name="android:text" />
        <attr name="weightShowRate" />
    </declare-styleable>
    <declare-styleable name="CYTextView">
        <attr name="mytextSize" format="dimension" />
        <attr name="mytextColor" format="reference|color" />
        <attr name="lineSpacingExtra" format="dimension" />
        <attr name="typeface" format="dimension" />
    </declare-styleable>
<declare-styleable name="QueryView">
         <attr name="isUseUserLayout" format="string"/>
     </declare-styleable>


    <declare-styleable name="NewLabelTextView">
        <attr name="NL_labelText" format="string" />
        <attr name="NL_valueText" format="string" />
        <attr name="android:ellipsize" />
        <attr name="NL_LableTextColor">
            <enum name="red" value="0"></enum>
            <enum name="black" value="1"></enum>
        </attr>
        <attr name="NL_ValueTextColor">
            <enum name="red" value="0"></enum>
            <enum name="black" value="1"></enum>
        </attr>
        <attr name="NL_weightShowRate" format="string" />
    </declare-styleable>
    <declare-styleable name="SelectGridView">
        <attr name="radioTrue" format="boolean" />
    </declare-styleable>

    <!-- 四方k线图组件 -->
    <declare-styleable name="ChartsView">
        <attr name="menu_margin" format="integer" />
        <attr name="menu_child_width" format="integer" />
        <attr name="menu_child_background" format="reference|color" />
        <attr name="menu_child_alpha" format="float" />
        <attr name="more_button" format="reference" />
        <attr name="fold_button" format="reference" />
        <attr name="left_arrow" format="reference" />
        <attr name="right_arrow" format="reference" />
        <attr name="zoom_in" format="reference" />
        <attr name="zoom_out" format="reference" />
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <!--星星间距-->
        <attr format="dimension" name="starDistance"/>
        <!--星星大小-->
        <attr format="dimension" name="starSize"/>
        <!--星星个数-->
        <attr format="integer" name="starCount"/>
        <!--星星空图-->
        <attr format="reference" name="starEmpty"/>
        <!--星星满图-->
        <attr format="reference" name="starFill"/>
    </declare-styleable>
     
</resources>