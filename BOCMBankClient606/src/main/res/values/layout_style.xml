<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="dialog_normal" parent="@android:style/Theme.Dialog">
    <item name="android:windowFrame">@null</item>
    <item name="android:windowIsFloating">true</item>
    <item name="android:windowIsTranslucent">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:backgroundDimEnabled">true</item>
    <!-- activity变暗 -->
    <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
  </style>


  <!--  距离左侧边距-->
    <dimen name="marginLeft">26dp</dimen>
    <!--NewLabelTextView  两个文本之间的间距-->
    <dimen name="textSplid_space">86dp</dimen>




  <!--分割线样式-->
  <style name="divider_vertical">
    <item name="android:layout_height">match_parent</item>
    <item name="android:layout_width">@dimen/boc_divider_1px</item>
    <item name="android:background">@color/boc_divider_line_color</item>
  </style>

  <style name="divider_horizontal">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">@dimen/boc_divider_1px</item>
    <item name="android:background">@color/boc_divider_line_color</item>
  </style>




</resources>
