<?xml version="1.0" encoding="utf-8"?>
<!-- listViewçš„headerView -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"    
	android:layout_height="@dimen/bovinvt_listview_height"
    android:orientation="horizontal" 
    android:gravity="center_vertical"
    >
    <TextView 
        android:id="@+id/tv_1"
        style="@style/style_tv_bocinv_agreement_appley"
        android:gravity="center_horizontal"
        android:text="@string/bocinvt_tv_14"
        />
    <TextView 
        android:id="@+id/tv_2"
        style="@style/style_tv_bocinv_agreement_appley"
        android:gravity="center_horizontal"
        android:text="@string/bocinvt_tv_54"
        />
    <TextView 
        android:id="@+id/tv_3"
        style="@style/style_tv_bocinv_agreement_appley"
        android:gravity="center_horizontal"
        android:text="@string/bocinvt_tv_15"
        />
    <View 
        android:layout_width="@dimen/bovinvt_layout_content_margin_top"
        android:layout_height="@dimen/bovinvt_layout_content_margin_top"
        android:contentDescription="@string/bocinvt_agreement"
        />

</LinearLayout>
