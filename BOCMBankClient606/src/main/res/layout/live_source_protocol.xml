<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/list_bottom" >
	<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/sbremit_fill_padding_left"
        android:background="@drawable/message_dialog_bg" >

        <ImageView
            android:id="@+id/img_exit_accdetail_1"
            android:layout_width="@dimen/btn_dialog_height"
            android:layout_height="@dimen/btn_dialog_height"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:src="@color/transparent_00" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/btn_close_height"
            android:layout_alignBottom="@+id/img_exit_accdetail_1"
            android:layout_toLeftOf="@+id/img_exit_accdetail_1"
            android:orientation="horizontal"
            android:visibility="invisible" >
        </LinearLayout>
    <LinearLayout
        android:id="@+id/contract_title_tv_ll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp_one_zero"
        android:orientation="vertical"
        android:paddingLeft="@dimen/fill_padding"
        android:paddingRight="@dimen/fill_padding_right" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="50dp"
            android:src="@drawable/bank_live" />

        <TextView
            android:id="@+id/contract_title_tv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/fill_margin_top"
            android:layout_marginTop="@dimen/fill_margin_top"
            android:gravity="center"
            android:text="中国银行缤纷生活"
            android:textSize="@dimen/textsize_one_six"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginTop="10dp"
            android:background="@color/gray_line"
            android:orientation="horizontal" >
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            
            >
            
            
       
		 <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:layout_below="@+id/tv_ll"
            android:layout_marginRight="@dimen/fill_margin_right_fordialog"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="@dimen/dp_five" >
        
        <WebView
            android:id="@+id/webView1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/contract_title_tv_ll"
            android:layout_marginTop="@dimen/common_row_margin"
            android:cacheColorHint="#00000000"
            android:paddingBottom="@dimen/dp_two_zero" />
		</LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginTop="10dp"
            android:background="@color/gray_line"
            android:orientation="horizontal" >
        </LinearLayout>
        <TextView
            android:id="@+id/down_live"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/fill_margin_top"
            android:layout_marginTop="@dimen/fill_margin_top"
            android:gravity="center"
            android:text="去下载"
            android:textColor="@color/blue"
            android:textSize="@dimen/textsize_one_six" />
    </LinearLayout>
     </LinearLayout>
</RelativeLayout>
    <ImageView
        android:id="@+id/img_exit_accdetail"
        android:layout_width="@dimen/close_clickarea_width"
        android:layout_height="@dimen/close_clickarea_height"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:src="@color/transparent_00" />

</RelativeLayout>