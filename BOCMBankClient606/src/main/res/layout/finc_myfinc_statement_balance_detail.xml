<?xml version="1.0" encoding="utf-8"?>
<!-- 基金 对账单详情 页面  fsm -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        style="@style/scrollview_comm"
        android:layout_marginBottom="@dimen/common_row_margin"
        android:layout_marginTop="@dimen/common_details_messages_margintop"
        android:background="@drawable/bg_for_listview_item_write" >

        <ScrollView
            android:id="@+id/prms_scrollView1_layout"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/common_details_messages_paddingtop" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
                
                <LinearLayout 
                    style="@style/linnerlayout_common"  >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_fundcode_colon" />

                    <TextView
                        android:id="@+id/finc_fundcode_tv"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

                <LinearLayout
                    style="@style/linnerlayout_common"  >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_fundname_colon" />

                    <TextView
                        android:id="@+id/finc_fundname_tv"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
				
                 <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_tradeCurrency_colon" />

                    <TextView
                        android:id="@+id/finc_tradecurrency_tv"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                
                 <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_totalBalance" />

                    <TextView
                        android:id="@+id/finc_myfinc_totalBalance"
                        android:textColor="@color/red"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" 
                    android:visibility="gone">

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_freezeBlance" />

                    <TextView
                        android:id="@+id/finc_myfinc_freezeBlance"
                        android:textColor="@color/red"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_availableBalance" />

                    <TextView
                        android:id="@+id/finc_myfinc_availableBalance"
                        android:textColor="@color/red"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_bounds" />

                    <TextView
                        android:id="@+id/finc_myfinc_bounds"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_netvalue_colon" />

                    <TextView
                        android:id="@+id/finc_myfinc_netPrice"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_netPriceDate" />

                    <TextView
                        android:id="@+id/finc_myfinc_netPriceDate"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                <LinearLayout 
                    style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_marketValue" />

                    <TextView
                        android:id="@+id/finc_myfinc_marketValue"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_floatLoss" />

                    <TextView
                        android:id="@+id/finc_myfinc_floatLoss"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

<!--                 <LinearLayout style="@style/linnerlayout_common" > -->
	
<!-- 	                <TextView android:id="@+id/fundCompanyInfo" -->
<!-- 	                    style="@style/finc_confirm_info_textview_style" -->
<!-- 	                    android:textSize="@dimen/textsize_one_two" -->
<!-- 	                    android:text="@string/finc_myfinc_fundStatement_disclaimer" /> -->
<!-- 	            </LinearLayout> -->
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_details_title_height"
        android:layout_marginTop="@dimen/common_details_title_margintop" >

        <TextView
            android:layout_width="@dimen/common_details_title_width"
            android:layout_height="@dimen/common_details_title_height"
            android:layout_centerHorizontal="true"
            android:background="@drawable/bg_title"
            android:gravity="center"
            android:text="@string/finc_detail"
            android:textSize="@dimen/textsize_default" />
    </RelativeLayout>

</RelativeLayout>