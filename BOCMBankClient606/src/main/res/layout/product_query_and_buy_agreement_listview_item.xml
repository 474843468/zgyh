<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/bovinvt_listview_height"
    android:gravity="center_vertical"
     >
    <TextView 
        android:id="@+id/tv_1"
        style="@style/style_tv_bocinv_profit_compute"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2.5dp"
        android:gravity="center"
        android:text=""
        />
    <TextView 
        android:id="@+id/tv_2"
        style="@style/style_tv_bocinv_profit_compute"
        android:gravity="center"
        android:layout_marginLeft="2.5dp"
        android:layout_marginRight="2.5dp"
        android:text=""
        />
    <TextView 
        android:id="@+id/tv_3"
        style="@style/style_tv_bocinv_profit_compute"
        android:layout_marginLeft="2.5dp"
        android:gravity="center"
        android:text=""
        />
    <ImageView 
        android:layout_width="@dimen/bovinvt_layout_content_margin_top"
        android:layout_height="@dimen/bovinvt_layout_content_margin_top"
        android:layout_marginRight="5dp"
        android:src="@drawable/right_arrow"
        android:contentDescription="@string/bocinvt_agreement"
        />

</LinearLayout>
