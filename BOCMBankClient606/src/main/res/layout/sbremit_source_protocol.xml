<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/list_bottom" >

    <LinearLayout
        android:id="@+id/contract_title_tv_ll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp_one_zero"
        android:background="@drawable/bg_for_listview_item_write"
        android:orientation="vertical"
        android:paddingLeft="@dimen/fill_padding"
        android:paddingRight="@dimen/fill_padding_right" >

        <TextView
            android:id="@+id/contract_title_tv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/fill_margin_top"
            android:layout_marginTop="@dimen/fill_margin_top"
            android:gravity="center"
            android:text="@string/sbremit_protocol_cotent_title"
            android:textColor="@color/gray"
            android:textSize="@dimen/textsize_one_six" />

        <WebView
            android:id="@+id/webView1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/contract_title_tv_ll"
            android:layout_marginTop="@dimen/common_row_margin"
            android:cacheColorHint="#00000000"
            android:paddingBottom="@dimen/dp_two_zero" />
    </LinearLayout>

</RelativeLayout>