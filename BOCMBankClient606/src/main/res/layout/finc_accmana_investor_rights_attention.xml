<?xml version="1.0" encoding="utf-8"?>
<!-- 基金 对账单详情 页面  fsm -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        style="@style/scrollview_comm"
        android:layout_marginBottom="@dimen/common_row_margin"
        android:layout_marginTop="@dimen/common_details_messages_margintop"
        android:background="@drawable/bg_for_listview_item_write" >

        <ScrollView
            android:id="@+id/prms_scrollView1_layout"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" 
                android:padding="@dimen/dp_one_zero">
                <TextView android:id="@+id/content"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_default"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</RelativeLayout>