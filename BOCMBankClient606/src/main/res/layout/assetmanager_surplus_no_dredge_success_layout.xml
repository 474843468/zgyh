<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/style_match_match"
    android:orientation="vertical" xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout 
        style="@style/style_height_weight_1"
        android:orientation="vertical"
        android:padding="10dp"
        >
        <TextView 
            style="@style/textview_common_multi_second"
            android:layout_width="match_parent"
            android:background="@drawable/shape_round_1"
            android:padding="10dp"
            android:text="@string/assetmanager_capital_39"
            />
        
        <LinearLayout 
            style="@style/style_match_wrap"
            android:background="@drawable/shape_round_1"
            android:layout_marginTop="10dp"
            android:padding="10dp"
            android:orientation="vertical"
            >
            <com.chinamworld.bocmbci.userwidget.LabelTextView
                style="@style/style_wrap_wrap"
                app:labelText="万份收益（元）："
                android:id="@+id/label_1"
                />
            <com.chinamworld.bocmbci.userwidget.LabelTextView
                style="@style/style_wrap_wrap"
                app:labelText="七日年化收益率"
                android:id="@+id/label_2"
                />
            <TextView 
                android:id="@+id/tv_1"
                style="@style/textview_common_multi_second"
                android:text="asldkjgkj"
                />
            
        </LinearLayout>
        
    </LinearLayout>
    
    
    <include layout="@layout/assetmanager_bottom_button_layout"/>

</LinearLayout>
