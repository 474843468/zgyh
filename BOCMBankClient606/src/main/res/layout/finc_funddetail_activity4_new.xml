<?xml version="1.0" encoding="utf-8"?>
<!-- 基金详情：赎回属性页面 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_for_listview_item_write"
        android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="#f5f5f5"
                    android:orientation="horizontal" >

                    <LinearLayout style="@style/linearlayout_common_outlay_detail" >

                        <TextView
                            style="@style/textview_common_outlay_detail606"
                            android:text="@string/finc_sellLowLimit_colon"
                             />

                        <TextView
                            android:id="@+id/tv_finc_sellLowLimit"
                            style="@style/bond_textview_common_outlay_detail606" />
                    </LinearLayout>

                    <View
                        android:id="@+id/v1"
                        android:layout_width="1dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="@dimen/common_row_margin"
                        android:background="@color/outlay_line" />

                    <LinearLayout style="@style/linearlayout_common_outlay_detail"
                        android:paddingRight="@dimen/common_row_margin" >

                        <TextView
                            style="@style/textview_common_outlay_detail606"
                            android:text="@string/finc_myfinc_holdQutyLowLimit"
                             />

                        <TextView
                            android:id="@+id/tv_finc_myfinc_holdQutyLowLimit"
                            style="@style/bond_textview_common_outlay_detail606" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/common_row_margin"
                    android:layout_marginRight="@dimen/common_row_margin"
                    android:background="@color/outlay_line" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="#f5f5f5"
                    android:orientation="horizontal" >

                    <LinearLayout style="@style/linearlayout_common_outlay_detail" >

                        <TextView
                            style="@style/textview_common_outlay_detail606"
                            android:text="@string/finc_myfinc_day_toplimit"
                             />

                        <TextView
                            android:id="@+id/tv_finc_myfinc_day_toplimit"
                            style="@style/bond_textview_common_outlay_detail606" />
                    </LinearLayout>

                    <View
                        android:layout_width="1dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="@dimen/common_row_margin"
                        android:background="@color/outlay_line" />

                    <LinearLayout style="@style/linearlayout_common_outlay_detail"
                        android:id="@+id/finc_lately_can_ransom_ll"
                        android:paddingRight="@dimen/common_row_margin"
                        android:visibility="invisible" >

                        <TextView
                            android:id="@+id/appendStrating"
                            style="@style/textview_common_outlay_detail606"
                            android:text="@string/finc_lately_can_ransom"
                             />

                        <TextView
                            android:id="@+id/tv_finc_lately_can_ransom"
                            style="@style/bond_textview_common_outlay_detail606" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:background="@color/outlay_line" />
                
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="#f5f5f5"
                    android:visibility="invisible"
                    android:orientation="horizontal" >

                    <LinearLayout style="@style/linearlayout_common_outlay_detail" >

                        <TextView
                            style="@style/textview_common_outlay_detail606"
                            android:text="@string/finc_myfinc_day_toplimit"
                             />

                        <TextView
                            style="@style/bond_textview_common_outlay_detail606" />
                    </LinearLayout>

                    <View
                        android:layout_width="1dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="@dimen/common_row_margin"
                        android:background="@color/outlay_line" />

                    <LinearLayout style="@style/linearlayout_common_outlay_detail"
                        android:visibility="invisible" >

                        <TextView
                            style="@style/textview_common_outlay_detail606"
                            android:text="@string/finc_lately_can_ransom"
                             />

                        <TextView
                            style="@style/bond_textview_common_outlay_detail606" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
    </LinearLayout>

</RelativeLayout>