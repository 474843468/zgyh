<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/style_match_match"
    android:orientation="vertical" 
    android:background="@color/white"
    >
    <LinearLayout 
        style="@style/style_height_weight_1"
        android:orientation="vertical" 
    	android:background="@color/white"
    	android:id="@+id/layout_content"
        >
        
    <TextView 
        android:id="@+id/tv_system_titile"
        style="@style/textview_common_multi_second"
        android:layout_width="match_parent"
        android:layout_gravity="center_horizontal"
        android:paddingLeft="20dp"
        android:background="#F2F2F2"
        android:text="@string/assetmanager_capital_45"
        />
    <ListView 
        android:id="@+id/listview"
        style="@style/listviewcomm_wrap_noDivider"
        android:background="@color/white"
        android:layout_marginLeft="20dp"
        />
    
    <!-- <include android:id="@+id/includ_1" layout="@layout/assetmanager_scene_include_layout"/> -->
    
    <TextView 
        android:id="@+id/tv_custom_titile"
        style="@style/textview_common_multi_second"
        android:layout_width="match_parent"
        android:layout_gravity="center_horizontal"
        android:paddingLeft="20dp"
        android:background="#F2F2F2"
        android:text="@string/assetmanager_capital_46"
        />
    
    <ListView 
        android:id="@+id/listview_custom"
        style="@style/listviewcomm_wrap_noDivider"
        android:background="@color/white"
        android:layout_marginLeft="20dp"
        />
    </LinearLayout>
    
    <include 
        android:id="@+id/includ_btn"
        layout="@layout/assetmanager_bottom_button_layout"
        />

</LinearLayout>
