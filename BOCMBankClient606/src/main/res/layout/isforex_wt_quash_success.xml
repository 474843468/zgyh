<?xml version="1.0" encoding="utf-8"?>
<!-- 撤单确认信息页面 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/fill_margin_bottom" >

    <LinearLayout
        android:id="@+id/ll_step"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <include layout="@layout/step_title_layout" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/btn_sure"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="@dimen/fill_margin_top" >

        <Button
            android:id="@+id/sureButton"
            style="@style/textview_forex_button_cell"
            android:text="@string/finish" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/btn_sure"
        android:layout_below="@+id/ll_step"
        android:layout_marginLeft="@dimen/fill_margin_left"
        android:layout_marginRight="@dimen/fill_margin_left"
        android:layout_marginTop="@dimen/fill_margin_top"
        android:background="@drawable/bg_for_listview_item_write"
        android:orientation="vertical" >

        <ScrollView
            style="@style/scrollview_comm_havemarginbutton"
            android:fillViewport="false"
            android:paddingLeft="0dp"
            android:paddingRight="0dp" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    style="@style/isforex_little_cell_title"
                    android:text="@string/forex_rate_quash_succ"
                     />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/common_row_margin" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/forex_rate_currency_number1" />

                        <TextView
                            android:id="@+id/forex_rate_currency_number1"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_result11" />

                        <TextView
                            android:id="@+id/forex_trade_number"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_vfgRegCurrency1" />

                        <TextView
                            android:id="@+id/forex_trade_sell"
                            style="@style/textview_forex_prompt_info"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_result21" />

                        <TextView
                            android:id="@+id/forex_trade_sell_code"
                            style="@style/textview_forex_prompt_info"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_sellTag" />

                        <TextView
                            android:id="@+id/forex_trade_buy"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_openPositionFlag" />

                        <TextView
                            android:id="@+id/forex_trade_code"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_money" />

                        <TextView
                            android:id="@+id/forex_trade_type"
                            style="@style/textview_forex_prompt_info"
                            android:textColor="@color/fonts_pink" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal"
                        android:visibility="gone" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_wtRate" />

                        <TextView
                            android:id="@+id/forex_trade_times"
                            style="@style/textview_forex_prompt_info"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_result31" />

                        <TextView
                            android:id="@+id/forex_trade_wtType"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/forex_trade_type" />

                        <TextView
                            android:id="@+id/forex_trade_type1"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_qudao" />

                        <TextView
                            android:id="@+id/forex_trade_jyQudao"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_times" />

                        <TextView
                            android:id="@+id/forex_trade_wtTimes"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_times1" />

                        <TextView
                            android:id="@+id/forex_trade_sxTimes"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_row_margin"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            android:text="@string/isForex_query_times2" />

                        <TextView
                            android:id="@+id/forex_trade_cdTimes"
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fill_margin_top"
                        android:orientation="horizontal"
                        android:visibility="invisible" >

                        <TextView
                            style="@style/textview_forex_prompt_message"
                            />

                        <TextView
                            style="@style/textview_forex_prompt_info"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</RelativeLayout>