<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_acc_out_listDetail"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/ll_acc_out_mytransfer"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_five"
        android:paddingLeft="@dimen/dp_one_zero"
        android:paddingRight="@dimen/dp_one_zero"
        android:paddingTop="@dimen/dp_five" >

        <!-- android:background="@drawable/tran_bg_acc" -->

        <LinearLayout
            android:id="@+id/ll_acc_out_2dimen_scan_write"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:visibility="gone" >

            <ImageView
                android:id="@+id/imageview"
                android:layout_width="20dip"
                android:layout_height="20dip"
                android:src="@drawable/tran_acc_out_person" />

            <TextView
                android:id="@+id/tran_flag_tv"
                style="@style/textview_for_list"
                android:layout_width="80dip"
                android:layout_height="20dip"
                android:layout_marginLeft="5dip"
                android:gravity="bottom"
                android:text="@string/tran_acc_out_person" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/dept_out_info_layout"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/ll_acc_out_2dimen_scan_write"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_nickName_accOut_list_detail"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:singleLine="true"
                    android:textColor="@color/card_top_text"
                    android:textSize="@dimen/textsize_one_for" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_bankaccount"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/card_three_row_paddingTop"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_accountType_accOut_list_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_for" />

                <TextView
                    android:id="@+id/tv_accountNumber_accOut_list_detail"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:singleLine="true"
                    android:textColor="@color/red"
                    android:textSize="@dimen/textsize_one_for" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/tv_crrey_accOut_layout_one"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/card_three_row_paddingTop"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_currey_accOut_list_detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_for" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/layout_margin_top"
                    android:layout_weight="4"
                    android:singleLine="true"
                    android:text="@string/acc_query_balance"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_two"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_lastprice_accOut_list_detail"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:textColor="@color/red"
                    android:textSize="@dimen/textsize_one_for" />
            </LinearLayout>
            
               <LinearLayout
                android:id="@+id/tv_crrey_accOut_layout_two"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/card_three_row_paddingTop"
                android:gravity="center_vertical"
                android:orientation="horizontal" 
                android:visibility="gone"
                >

                <TextView
                    android:id="@+id/tv_currey_accOut_list_detail_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_for" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/layout_margin_top"
                    android:layout_weight="4"
                    android:singleLine="true"
                    android:text="@string/acc_query_balance"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_two"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_lastprice_accOut_list_detail_two"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:textColor="@color/red"
                    android:textSize="@dimen/textsize_one_for" />
            </LinearLayout>
            
             <LinearLayout
                android:id="@+id/tv_crrey_accOut_layout_three"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/card_three_row_paddingTop"
                android:gravity="center_vertical"
                android:orientation="horizontal" 
                android:visibility="gone"
                >

                <TextView
                    android:id="@+id/tv_currey_accOut_list_detail_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_for" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/layout_margin_top"
                    android:layout_weight="4"
                    android:singleLine="true"
                    android:text="@string/acc_query_balance"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textsize_one_two"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_lastprice_accOut_list_detail_three"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:textColor="@color/red"
                    android:textSize="@dimen/textsize_one_for" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_gone_tran"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/dept_out_info_layout"
            android:gravity="center_horizontal"
            android:paddingTop="0dip"
            android:visibility="gone" >

            <ImageView
                android:layout_width="20dip"
                android:layout_height="20dip"
                android:src="@drawable/tran_acc_hand" />

            <TextView
                style="@style/textview_for_list"
                android:layout_width="80dip"
                android:layout_height="20dip"
                android:layout_marginLeft="5dip"
                android:gravity="bottom"
                android:text="@string/tran_select_again" />
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>