<?xml version="1.0" encoding="utf-8"?>
<!-- 成交状况查询结果页面  itme -->

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/common_click_item_style"
    android:paddingLeft="0dp"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:id="@+id/layout_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/isForex_result_number"
            style="@style/bond_textview_15_black_1"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/btn_bottom_height"
            android:layout_weight="1"
            android:gravity="left|center"
            android:singleLine="true"
            android:text="5465657" />

        <TextView
            android:id="@+id/isForex_result_code"
            style="@style/bond_textview_15_black_1"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/btn_bottom_height"
            android:layout_weight="1"
            android:gravity="center"
            android:singleLine="true"
            android:text="5465657" />

        <TextView
            android:id="@+id/isForex_result_type"
            style="@style/bond_textview_15_black_1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="right"
            android:singleLine="true"
            android:text="6566576576"
            android:textColor="@color/fonts_pink" />

        <ImageView
            android:id="@+id/rate_gotoDetail"
            style="@style/rightbtn_jiantou"
            android:layout_gravity="center"
            android:gravity="center"
            android:src="@drawable/right_arrow" />
    </LinearLayout>

</RelativeLayout>