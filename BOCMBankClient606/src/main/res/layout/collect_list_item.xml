<?xml version="1.0" encoding="utf-8"?>
<!-- 跨行资金归集被归集列表 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:id="@+id/body"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:background="@drawable/selector_for_click_list_item"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/fill_padding_top"
        android:paddingLeft="@dimen/layout_margin_top"
        android:paddingTop="@dimen/fill_padding_top" >

        <CheckBox
            android:id="@+id/cb_select"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/checkbox_selector"
            android:button="@null"
            android:focusable="false" />

        <TextView
            android:id="@+id/tv_one_item"
            style="@style/textview_common"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:gravity="center"
            android:singleLine="true"
            android:text="--"
            android:textSize="@dimen/textsize_one_two" />

       

        <TextView
            android:id="@+id/tv_three_item"
            style="@style/textview_common"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:gravity="center"
            android:singleLine="true"
            android:text="--"
            android:textSize="@dimen/textsize_one_two" />

         <TextView
            android:id="@+id/tv_two_item"
            style="@style/textview_common"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:gravity="center"
            android:singleLine="true"
            android:text="--"
            android:textSize="@dimen/textsize_one_two" />
         
        <ImageView
            android:id="@+id/rate_gotoDetail"
            android:layout_width="@dimen/shap_of_commrequestopen_pdl"
            android:layout_height="@dimen/shap_of_commrequestopen_pdl"
            android:layout_gravity="center"
            android:gravity="center"
            android:src="@drawable/right_arrow" />
    </LinearLayout>

    <View
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/body"
        android:layout_alignBottom="@+id/body"
        android:background="@color/transparent_00" />

</RelativeLayout>