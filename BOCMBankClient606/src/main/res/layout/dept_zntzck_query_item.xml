<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_six_zero"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingBottom="@dimen/dp_five"
        android:paddingLeft="@dimen/common_row_margin"
        android:paddingTop="@dimen/dp_five" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_row_margin"
            android:orientation="horizontal" >

            <TextView
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/dept_zntzck_query_signAcc" />

            <TextView
                android:id="@+id/dept_zntzck_query_signAcc"
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="46546" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_row_margin"
            android:orientation="horizontal" >

            <TextView
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/dept_zntzck_query_signType" />

            <TextView
                android:id="@+id/dept_zntzck_query_signType"
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="dgfdh" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_row_margin"
            android:orientation="horizontal" >

            <TextView
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/dept_zntzck_query_signDate" />

            <TextView
                android:id="@+id/dept_zntzck_query_signDate"
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/cancelChnlFlag_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_row_margin"
            android:orientation="horizontal"
            android:visibility="visible" >

            <TextView
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dept_zntzck_query_cancelChnlFlag" />

            <TextView
                android:id="@+id/dept_zntzck_query_cancelChnlFlag"
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/cancelDate_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_row_margin"
            android:orientation="horizontal"
            android:visibility="visible" >

            <TextView
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dept_zntzck_query_cancelDate" />

            <TextView
                android:id="@+id/dept_zntzck_query_cancelDate"
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_row_margin"
            android:orientation="horizontal" >

            <TextView
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/dept_zntzck_query_signStatus" />

            <TextView
                android:id="@+id/dept_zntzck_query_signStatus"
                style="@style/textview_common"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

    <!--
    	android:layout_width="@dimen/dp_five_zero"
        android:layout_height="@dimen/btn_dialog_height"   -->
    <Button
        android:id="@+id/dept_dqyzc_detail_cancel"
        android:layout_width="@dimen/btn_close_width"
        android:layout_height="@dimen/btn_close_height"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:layout_marginRight="@dimen/common_row_margin"
        android:background="@drawable/btn_goumai"
        android:text="@string/dept_zntzck_query_cancel"
        android:textColor="@color/white"
        android:textSize="@dimen/textsize_default" />

</RelativeLayout>