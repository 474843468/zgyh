<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/style_match_wrap"
    android:orientation="vertical" 
    android:paddingTop="10dp"
    android:paddingBottom="20dp"
    android:paddingLeft="3dp"
    android:paddingRight="3dp"
    android:background="@drawable/shape_round"
    >
    <com.chinamworld.bocmbci.userwidget.LabelTextView
        style="@style/style_match_wrap"
        app:labelText="划转日期："
        app:weightShowRate="2:3"
        android:id="@+id/vh_label_tv_1"
        />
    <com.chinamworld.bocmbci.userwidget.LabelTextView
        style="@style/style_match_wrap"
        app:labelText="划转金额："
        app:weightShowRate="2:3"
        app:ValueTextColor="red"
        android:id="@+id/vh_label_tv_2"
        />
    <com.chinamworld.bocmbci.userwidget.LabelTextView
        style="@style/style_match_wrap"
        app:labelText="划转状态："
        app:weightShowRate="2:3"
        android:id="@+id/vh_label_tv_3"
        />
    <com.chinamworld.bocmbci.userwidget.LabelTextView
        style="@style/style_match_wrap"
        app:labelText="错误码："
        app:weightShowRate="2:3"
        android:id="@+id/vh_label_tv_4"
        />
    <com.chinamworld.bocmbci.userwidget.LabelTextView
        style="@style/style_match_wrap"
        app:labelText="备注："
        app:weightShowRate="2:3"
        android:id="@+id/vh_label_tv_5"
        />
    

</LinearLayout>
