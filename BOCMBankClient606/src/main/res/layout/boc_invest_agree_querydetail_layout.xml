<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/agreequery_ll_two"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <com.chinamworld.bocmbci.userwidget.TitleAndContentLayout
            android:id="@+id/titleAndContentLayout_detailinfo"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >
        </com.chinamworld.bocmbci.userwidget.TitleAndContentLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/boci_agree_touzi_list"
                style="@style/style_boci_xpadquery_btn"
                android:visibility="visible"
                android:text="投资明细查询" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>