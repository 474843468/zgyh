<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <!-- 非净值型中银理财产品 赎回页面与赎回确认页面相同的部分 -->

    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_product_code"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/bocinvt_product_code" />

    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_product_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/bocinvt_product_name" />

    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_currency"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/curCode" />
    
    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_cashRemit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/bocinvt_tv_40" />

    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_redeem_worth"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/bocinvt_redeem_worth" />

    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_hold_quantity_min"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/bocinvt_redeem_hold_quantity_min" />

    <com.chinamworld.bocmbci.userwidget.LabelTextView
        android:id="@+id/ltv_redeem_start_quantity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:labelText="@string/bocinvt_redeem_starting_quantity" />

</merge>