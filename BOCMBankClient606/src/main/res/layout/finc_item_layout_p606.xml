<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:orientation="horizontal"
    android:background="@color/boc_common_cell_color"

    android:id="@+id/finc_list_title_layout">

    <LinearLayout
        android:id="@+id/left_item_layout"
        android:layout_width="136dp"
        android:layout_height="60dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="visible">
        <TextView
            android:id="@+id/left_container_textview0"
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:gravity="left"
            android:paddingTop="15dp"
            android:singleLine="true"
            style="@style/finc_text_blod_black_style13"
            android:text="--"/>
        <TextView
            android:id="@+id/finc_code"
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:gravity="left"
            style="@style/finc_text_blod_black_style13"
            android:text="--"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/right_item_layout"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:visibility="visible">
    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/item_title_tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="21dp"
            android:paddingRight="33dp"
            android:gravity="right"
            style="@style/finc_text_blod_black_style13"
            android:text="--"/>
        <TextView
            android:id="@+id/tv_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:paddingRight="33dp"
            android:layout_gravity="center_horizontal"
            android:textSize="10sp"
            android:textColor="@color/fonts_black"
            android:text="--"/>

    </LinearLayout>

    <TextView
        android:id="@+id/item_title_tv3"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingTop="21dp"
        android:text="--"
        android:gravity="center_horizontal"
        style="@style/finc_text_blod_black_style13" />
    <TextView
        android:id="@+id/item_title_tv4"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingTop="21dp"
        android:text="--"
        android:gravity="center_horizontal"
        style="@style/finc_text_blod_black_style13"/>
    <TextView
        android:id="@+id/item_title_tv5"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingTop="21dp"
        android:text="--"
        android:gravity="center_horizontal"
        style="@style/finc_text_blod_black_style13"/>
    <TextView
        android:id="@+id/item_title_tv6"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingTop="21dp"
        android:text="--"
        android:gravity="center_horizontal"
        style="@style/finc_text_blod_black_style13"/>
    <TextView
        android:id="@+id/item_title_tv7"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingTop="21dp"
        android:text="--"
        android:gravity="center_horizontal"
        style="@style/finc_text_blod_black_style13"/>
    <TextView
        android:id="@+id/item_title_tv8"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingTop="21dp"
        android:text="--"
        android:gravity="center_horizontal"
        style="@style/finc_text_blod_black_style13"/>
    </LinearLayout>

</LinearLayout>
