<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content">

	<LinearLayout android:id="@+id/ll_acc_out_2dimen_scan_write"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:gravity="center_horizontal" android:paddingTop="@dimen/fill_padding_top">

		<ImageView android:layout_width="@dimen/btn_hide_width"
			android:layout_height="@dimen/btn_hide_width" android:src="@drawable/tran_acc_out_person" />

		<TextView android:layout_width="wrap_content"
			android:layout_height="@dimen/btn_hide_width"
			android:layout_marginLeft="@dimen/dp_five" android:gravity="bottom"
			android:text="@string/thrid_cecuritytrade_cecuracc" />
	</LinearLayout>

	<LinearLayout android:id="@+id/dept_out_info_layout"
		android:layout_width="fill_parent" android:layout_height="@dimen/titlebackbtn_width"
		android:layout_below="@+id/ll_acc_out_2dimen_scan_write"
		android:orientation="vertical">

		
	</LinearLayout>

	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_below="@+id/dept_out_info_layout"
		android:layout_alignParentBottom="true"
		 android:paddingTop="@dimen/dp_five"
		android:gravity="center_horizontal" >

		<ImageView android:layout_width="@dimen/btn_hide_width"
			android:layout_height="@dimen/btn_hide_width"
			 android:src="@drawable/tran_acc_hand" />

		<TextView android:layout_width="@dimen/prms_obmessage_edittextheight"
			android:layout_height="@dimen/btn_hide_width"
			android:layout_marginLeft="@dimen/dp_five"
			android:gravity="bottom" android:text="@string/tran_select_again" />
	</LinearLayout>

</RelativeLayout>