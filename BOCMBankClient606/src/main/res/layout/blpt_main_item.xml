<?xml version="1.0" encoding="utf-8"?>
<!-- 账单缴费主页item -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_item"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_seven_zero" >

    <RelativeLayout
        android:id="@+id/back"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dp_seven_zero"
        android:tag="back" >

        <FrameLayout
            android:id="@+id/acc_frame_back"
            android:layout_width="@dimen/icon_dele_HW"
            android:layout_height="@dimen/icon_dele_HW"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" >

            <ImageView
                android:id="@+id/acc_btn_goitem_back"
                android:layout_width="@dimen/rightbtnW"
                android:layout_height="@dimen/rightbtnH"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/ll_acc_msg_back"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/dp_seven_zero"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/acc_frame_back"
            android:background="@drawable/img_ka_press"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/acc_list_item_height"
                android:text="@string/blpt_main_item_hint"
                android:textColor="@color/black" />
        </FrameLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/front"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_seven_zero" >

        <FrameLayout
            android:id="@+id/acc_frame"
            android:layout_width="@dimen/rightbtnW"
            android:layout_height="@dimen/rightbtnW"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" >

            <ImageView
                android:id="@+id/btn_goitem"
                style="@style/rightbtn_jiantou"
                android:layout_gravity="center"
                android:background="@drawable/img_arrow_write" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/ll_acc_msg"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/dp_seven_zero"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/acc_frame"
            android:background="@drawable/bg_for_listview_item_write"
            android:gravity="center" >

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dp_seven_zero" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignBottom="@+id/img_acc_detail"
                    android:layout_below="@+id/ll_bankact_top"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/icon_dele_HW_margin"
                    android:paddingRight="@dimen/icon_dele_HW_margin">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_company"
                            style="@style/textview_for_list"
                            android:textSize="@dimen/textsize_one_five"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <TextView
                            android:id="@+id/tv_dispname"
                            style="@style/textview_for_list_act"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:singleLine="true" />
                    </LinearLayout>

                </LinearLayout>
            </RelativeLayout>

            <ImageView
                android:id="@+id/imageViewright"
                style="@style/sucess_right_iv"
                android:layout_gravity="center"
                android:visibility="gone" />
        </FrameLayout>
    </RelativeLayout>

    <ImageView
        android:id="@+id/btn_gocancelrelation"
        android:layout_width="@dimen/icon_dele_HW"
        android:layout_height="@dimen/icon_dele_HW"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:background="@drawable/icon_del"
        android:visibility="gone" />

</RelativeLayout>

