<?xml version="1.0" encoding="utf-8"?>
<!-- 到期日期明细查询 详情 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <include
        android:id="@+id/stepbar"
        layout="@layout/step_title_layout" />

    <LinearLayout
        style="@style/scrollview_comm"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="@dimen/common_details_messages_paddingtop"
        android:background="@drawable/bg_for_listview_item_write"
        android:paddingBottom="@dimen/common_row_margin"
        android:paddingTop="@dimen/common_details_messages_paddingtop" >

        <ScrollView
            style="@style/scrollview_comm"
            android:scrollbars="none" >

            <LinearLayout
               android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/fill_padding_top"
                android:paddingLeft="@dimen/common_listview_item_paddingLR"
                android:paddingRight="@dimen/common_listview_item_paddingLR" >

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_fincCode" />

                    <TextView
                        android:id="@+id/finc_fincCode"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_fincName" />

                    <TextView
                        android:id="@+id/finc_finc_name"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_feetype_colon" />

                    <TextView
                        android:id="@+id/finc_feetype_colon"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_reality_share" />

                    <TextView
                        android:id="@+id/finc_reality_share"
                        style="@style/confirm_right_textview_style606"
                        android:textColor="@color/red" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_totalCount" />

                    <TextView
                        android:id="@+id/finc_myfinc_totalCount"
                        style="@style/confirm_right_textview_style606"
                        android:textColor="@color/red" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_register_date" />

                    <TextView
                        android:id="@+id/finc_myfinc_register_date"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>

                <LinearLayout style="@style/linnerlayout_common" >

                    <TextView
                        style="@style/confirm_left_textview_style606"
                        android:text="@string/finc_myfinc_redemption_date" />

                    <TextView
                        android:id="@+id/finc_myfinc_redemption_date"
                        style="@style/confirm_right_textview_style606" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

        <ImageView
            android:id="@+id/img_exit_accdetail"
            style="@style/Finc_Dialog_Close" />
    </LinearLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_details_title_height"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/common_details_title_margintop" >

        <TextView
            android:id="@+id/acc_financeic_trans_title"
            android:layout_width="@dimen/common_details_title_width"
            android:layout_height="@dimen/common_details_title_height"
            android:layout_centerHorizontal="true"
            android:background="@drawable/bg_title"
            android:gravity="center"
            android:text="@string/finc_myfinc_detail_1"
            android:textSize="@dimen/textsize_default" />
    </RelativeLayout>

</RelativeLayout>