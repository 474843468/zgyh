<?xml version="1.0" encoding="utf-8"?>
<!-- 账户管理  三级菜单项——医保账户  wangmengmeng -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:id="@+id/ll_menu"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_for_click_content"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/acc_financeic_account"
            style="@style/common_click_first_item_style"
            android:gravity="center" >

            <TextView
                style="@style/textview_common_right"
                android:gravity="left"
                android:text="@string/acc_medical_menu_1" />

            <ImageView
                style="@style/rightbtn_jiantou"
                android:background="@drawable/right_arrow"
                android:focusable="false" />
        </LinearLayout>

     
        <ImageView style="@style/divider_imag_style" />

        <LinearLayout
            android:id="@+id/acc_financeic_transfer_detail"
            style="@style/common_click_last_item_style"
            android:gravity="center" >

            <TextView
                style="@style/textview_common_right"
                android:text="@string/acc_finance_menu_3" />

            <ImageView
                style="@style/rightbtn_jiantou"
                android:background="@drawable/right_arrow"
                android:focusable="false" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>