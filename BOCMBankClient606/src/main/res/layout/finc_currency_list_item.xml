<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:orientation="horizontal"
    android:background="@color/boc_common_cell_color"
    android:id="@+id/finc_list_title_layout">

    <LinearLayout
        android:id="@+id/left_item_layout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="visible">
        <TextView
            android:id="@+id/finc_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:layout_gravity="center"
            style="@style/finc_text_blod_black_style13"
            android:text="--"/>
        <TextView
            android:id="@+id/finc_code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:layout_gravity="center"
            style="@style/finc_text_blod_black_style13"
            android:text="--"/>

    </LinearLayout>
    <TextView
        android:id="@+id/finc_week_profit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="--"
        android:paddingTop="21dp"
        android:paddingRight="15dp"
        android:gravity="right"
        style="@style/finc_text_blod_black_style13" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"

            android:orientation="vertical">
            <TextView
                android:id="@+id/finc_ten_thousand_profit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:paddingTop="21dp"
                android:paddingRight="30dp"
                android:layout_gravity="center"
                style="@style/finc_text_blod_black_style13"
                android:text="--"/>
            <TextView
                android:id="@+id/tv_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:paddingRight="30dp"
                android:layout_gravity="center"
                android:textSize="10sp"
                android:textColor="@color/fonts_black"
                android:text="--"/>

        </LinearLayout>

</LinearLayout>
