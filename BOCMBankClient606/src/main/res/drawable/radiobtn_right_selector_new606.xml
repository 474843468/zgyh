<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item

        android:state_checked="true">
        <shape>
            <corners android:topRightRadius="@dimen/titlerightbtn_paddingtb" android:bottomRightRadius="@dimen/titlerightbtn_paddingtb"/>
            <solid android:color="@color/fundp606_title"/>

        </shape>
    </item>
    <item android:state_checked="false">

        <shape>
            <corners android:topRightRadius="@dimen/titlerightbtn_paddingtb" android:bottomRightRadius="@dimen/titlerightbtn_paddingtb"/>
            <solid android:color="@color/white"/>
            <stroke android:width="1dp" android:color="@color/fundp606_title"/>
        </shape>
    </item>


</selector>