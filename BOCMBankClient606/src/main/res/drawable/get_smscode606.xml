<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!-- add by luqp 添加按钮样式 -->
    <item android:state_focused="false">
        <shape>
            <corners android:radius="@dimen/titlerightbtn_paddingtb"/>
            <solid android:color="@color/bg_table_row"/>
            <stroke android:width="1dp" android:color="@color/goldstore_blue" />
        </shape>
    </item>

    <item android:state_focused="true" >
        <shape>
            <corners android:radius="@dimen/titlerightbtn_paddingtb"/> <!-- 设置圆角 -->
            <solid android:color="@color/bg_table_row"/>
            <stroke android:color="@color/goldstore_blue" />
        </shape>
    </item>
</selector>


