<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/share_size_five"/>
            <solid android:color="@color/btn_common_oneline_right_press_color"/>
            <!-- 描边 -->
            <stroke android:width="0.5dp" android:color="#c8c8c8" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/share_size_five"/>
            <solid android:color="@color/btn_common_oneline_right_press_color"/>
            <!-- 描边 -->
            <stroke android:width="0.5dp" android:color="#c8c8c8" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/share_size_five"/>
            <solid android:color="@color/btn_common_oneline_right_normal_color"/>
            <!-- 描边 -->
            <stroke android:width="0.5dp" android:color="#c8c8c8" />
        </shape>
    </item>
</selector>
